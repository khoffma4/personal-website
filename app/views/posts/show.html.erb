<p id="notice"><%= notice %></p>

<h1 class="post-title">
  <%= @post.title %>
</h1>
<p>written on<%= @post.created_at.to_date %>

<p class="post-body">
  <%= @post.content %>
</p>

<%= link_to 'Back', posts_path %>

<% if session[:user_id] %>
<%= link_to "Edit", edit_post_url %>
<%= link_to 'Delete', @post, confirm: 'Are you sure?', method: :delete %>
<% end %>
